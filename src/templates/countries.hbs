{{#each this}}
<li class="card__country">
<h1 class="card__country-name" >{{name}}</h1> 
    <div class="card__container">
         <div class="card__text">
            <p ><span class="card__capital">Capital:</span> {{capital}}</p>
            <p ><span class="card__population">Population:</span>  {{population}}</p>
            <ul><span class="card__languages">Languages:</span> 
                {{#each languages}}
                    <li> {{this.name}}</li>
                {{/each}}
            </ul>
        </div>
        <div class="card__flag">
            <img src="{{flag}}" alt="{{name}}" width="520">
        </div>
    </div>
</li>
{{/each}}

